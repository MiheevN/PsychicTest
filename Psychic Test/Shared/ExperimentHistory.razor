@inherits InfoPanel

<h3 class="text-center">История экспериментов</h3>
<table class="table">
    <thead>
        <tr>
            <th>Номер эксперимента</th>
            <th>Введённое число</th>
            @foreach (var Psy in Group.Psyphics)
            {
                <th>Догадки @Psy.Name</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Group.EnteredNumbers.Count; i++)
        {
            <tr>
                <td>@(i+1)</td>
                <td>@Group.EnteredNumbers[i]</td>
                @foreach (var Psy in Group.Psyphics)
                {
                    <td class="@string.Format(Psy.Guesses[i] == Group.EnteredNumbers[i] ? "text-success" : "text-danger")">
                        @Psy.Guesses[i]
                    </td>
                }
            </tr>
        }
    </tbody>
</table>

